{
    "prolog.executablePath": "/usr/bin/swipl",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "tests/dev/**":true,
        "tests/stable/**":true
    },
    "todohighlight.isEnable": true,
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.pl"
    ],
    "spellright.language": [],
    "spellright.documentTypes": [
        "latex"
    ],
    "timelink.showAllFilesInExplorer": true,

}